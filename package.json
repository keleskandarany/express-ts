{
  "name": "express-ts",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d ./src/ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^4.21.1",
    "mysql2": "^3.11.3",
    "reflect-metadata": "^0.2.2",
    "remove": "^0.1.5",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.7",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.6.3"
  },
  "private": true
}
